    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc build and debug Project rail2 encrypt",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/bin/project",
            "args": ["rail2","encrypt","This is such a great message. Truly Tremendous.","7","4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc build project file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "gcc build and debug Project no args",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/bin/project",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc build project file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}